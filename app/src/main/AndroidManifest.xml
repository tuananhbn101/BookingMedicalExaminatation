<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookingmedicalexaminatation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookingMedicalExaminatation">
        <activity
            android:name=".view.profile.PatientActivity"
            android:exported="false" />
        <activity
            android:name=".view.profile.DoctorActivity"
            android:exported="false" />
        <activity
            android:name=".view.login_register.DoctorRegisterActivity"
            android:exported="false" />
        <activity
            android:name=".view.profile.PatientProfileActivity"
            android:exported="false" />
        <activity
            android:name=".view.profile.DoctorProfileActivity"
            android:exported="false" />
        <activity
            android:name=".view.bookappointment.BookAppointmentActivity"
            android:exported="false" />
        <activity
            android:name=".view.login_register.PatientRegisterActivity"
            android:exported="true" />
        <activity
            android:name=".view.login_register.LoginActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".view.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>